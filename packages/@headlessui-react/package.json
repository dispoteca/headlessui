{
  "name": "@headlessui/react",
  "version": "1.6.6",
  "description": "A set of completely unstyled, fully accessible UI components for React, designed to integrate beautifully with Tailwind CSS.",
  "main": "dist/index.cjs",
  "typings": "dist/index.d.ts",
  "module": "dist/headlessui.esm.js",
  "license": "MIT",
  "files": [
    "README.md",
    "dist"
  ],
  "exports": {
    "import": "./dist/headlessui.esm.js",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tailwindlabs/headlessui.git",
    "directory": "packages/@headlessui-react"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "npm run build",
    "prepublishOnly": "npm run build",
    "build": "npm-run-all build-cjs build-cjs-dev build-esm build-iife build-types copy-build",
    "build-cjs": "cross-env NODE_ENV=production esbuild ./src/index.ts --format=cjs --outfile=./dist/headlessui.prod.cjs.js --minify --bundle --platform=browser --target=es2020 --external:react --external:react-dom",
    "build-cjs-dev": "cross-env NODE_ENV=development esbuild ./src/index.ts --format=cjs --outfile=./dist/headlessui.dev.cjs.js --bundle --platform=browser --target=es2020 --external:react --external:react-dom",
    "build-esm": "cross-env NODE_ENV=production esbuild ./src/index.ts --format=esm --outfile=./dist/headlessui.esm.js --minify --bundle --platform=browser --target=es2020 --external:react --external:react-dom",
    "build-iife": "cross-env NODE_ENV=production esbuild ./src/index.ts --format=iife --outfile=./dist/headlessui.iife.js --minify --bundle --platform=browser --target=es2020 --external:react --external:react-dom",
    "build-types": "tsc --emitDeclarationOnly --outDir ./dist",
    "copy-build": "copyfiles -u 1 ./build/* ./dist",
    "watch": "../../scripts/watch.sh --external:react --external:react-dom",
    "test": "../../scripts/test.sh",
    "lint": "../../scripts/lint.sh",
    "playground": "yarn workspace playground-react dev",
    "clean": "rimraf ./dist"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "devDependencies": {
    "@testing-library/react": "^13.0.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.11.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "snapshot-diff": "^0.8.1"
  }
}
